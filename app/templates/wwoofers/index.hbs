<div class="page-header">
    <h1>
        Wwoofers
    </h1>
</div>

<table class="table table-hover">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Postal Code</th>
        <th>City</th>
        <th>Country</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {{#each}}
        <tr>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{postalCode}}</td>
            <td>{{city}}</td>
            <td>{{country}}</td>
            <td>
                <a href="#">Edit</a> |
                <a href="#">Delete</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
