<div class="page-header">
    <h1>
        Wwoofers
    </h1>
</div>

<table class="table table-hover">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Postal Code</th>
        <th>City</th>
        <th>Country</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {{#each}}
        <tr>
            <td>{{user.firstName}}</td>
            <td>{{user.lastName}}</td>
            <td>{{address.zipCode}}</td>
            <td>{{address.city}}</td>
            <td>{{address.country.name}}</td>
            <td>
                {{#link-to "wwoofer" id}}View{{/link-to}} |
                <a href="#">Edit</a> |
                <a href="#">Delete</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
