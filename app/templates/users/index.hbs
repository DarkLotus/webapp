<div class="page-header">
    <h1>
        Users
    </h1>
</div>

<table class="table table-hover">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Password</th>
        <th>Host?</th>
        <th>Wwoofer?</th>
    </tr>
    </thead>
    <tbody>
    {{#each}}
        <tr>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{username}}</td>
            <td>{{password}}</td>
            <td>
                {{#link-to "user" model}}View{{/link-to}} |
                <a href="#">Edit</a> |
                <a href="#">Delete</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
