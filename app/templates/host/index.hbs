<div class="page-header">
    <h1>{{farmName}}
        <small>{{contact}}</small>
    </h1>
</div>

<div class="row">
    <div class="col-md-6">

        <p>Address: {{address}}</p>

        <p>Zip Code: {{zipCode}}</p>

        <p>City: {{city}}</p>

        <hr/>

        <p>Email: {{email}}</p>

        <p>Phone:  {{phone}}</p>

        <p>Website: {{web}}</p>

        <hr/>

        <p>Entry: {{entry}}</p>

        <p>Travel:  {{travel}}</p>

    </div>
    <div class="col-md-6">
        <div class="host-gallery swipeshow">
            <ul class="slides">
                {{#each photos}}
                    <li class="slide">
                        <img {{bind-attr src="completeUrl"}} />
                    </li>
                {{else}}
                    <li class="slide">
                        <img src="wwoof-no-photo.png"/>
                    </li>
                {{/each}}
            </ul>
            <!--<div class='dots'></div>-->
        </div>
    </div>
</div>
